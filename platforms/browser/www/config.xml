<?xml version='1.0' encoding='utf-8'?>
<widget id="com.tecbiz.tecbizassociado" version="314" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>TecBiz Associado</name>
    <description>
        Aplicativo para consulta de saldos, extratos e lojas credenciadas.
    </description>
    <author email="tecnologia@tecbiz.com.br" href="http://www.tecbiz.com.br">
        TecBiz Tecnologia e Negocios
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="phonegap-version" value="cli-7.0.1" />
</widget>
